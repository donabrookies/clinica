{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "SUPABASE_URL": "https://seu-projeto.supabase.co",
    "SUPABASE_KEY": "sua-service-role-key-aqui",
    "ADMIN_EMAIL": "admin@clinica.com"
  }
}